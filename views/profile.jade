extends layout

block content
	section#main
		if user
			.centred
				img.round(src="#{user.picture}", width="200") 
				p= user.displayName
				p= user.emails[0].value
				p= user._json.app_metadata.customerId
				if user._json.app_metadata.roles
					each role in user._json.app_metadata.roles
						p= role